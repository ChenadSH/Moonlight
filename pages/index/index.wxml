<!--index.wxml-->
<view class="container">
  <view class='header-wrapper flex flex-jcc flex-ac'>
    <image class="header-image" src="../../images/network.png"></image>
  </view>
  <view class='content-wrapper'>
    <view class="title">选择操作</view>
    <view class="btn-info">
      <view bindtap="bindViewBlue" class="select-way flex-1 flex flex-v flex-jcc flex-ac">
        <text class="btn-icon iconfont icon-peiwang"></text>
        <text class="name">配网</text>
      </view>
      <view bindtap="bindViewWifi" class="select-way flex-1 flex flex-v flex-jcc flex-ac">
          <text class="btn-icon iconfont icon-kongzhi"></text>
          <text class="name">控制</text>
      </view>
    </view>
  </view>
</view>
